<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BottomNavigation">
        <attr name="bn_tab_icon_width" format="dimension" />
        <attr name="bn_tab_icon_height" format="dimension" />
        <attr name="bn_tab_text_size" format="dimension" />
        <attr name="bn_tab_text_color" format="color" />
        <attr name="bn_tab_text_marginTop" format="dimension" />
        <attr name="bn_tab_vp_smoothScroll" format="boolean" />
        <attr name="bn_tab_vp_limit" format="integer" />
    </declare-styleable>

    <declare-styleable name="GradientTabView">
        <attr name="gt_icon_bot_src" format="reference" />
        <attr name="gt_icon_top_src" format="reference" />
        <attr name="gt_icon_width" format="dimension" />
        <attr name="gt_icon_height" format="dimension" />
        <attr name="gt_text" format="string" />
        <attr name="gt_text_size" format="dimension" />
        <attr name="gt_text_bot_color" format="color" />
        <attr name="gt_text_top_color" format="color" />
        <attr name="gt_text_marginTop" format="dimension" />
    </declare-styleable>
</resources>